#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

#define ZMK_POINTING_DEFAULT_MOVE_VAL 1500  // default: 600
#define ZMK_POINTING_DEFAULT_SCRL_VAL 20    // default: 10

#include <dt-bindings/zmk/pointing.h>

#define BASE  0
#define NUM   1
#define FUNC  2
#define EXT   3
#define GAME  4
#define MOUSE 5

/ {
    behaviors {
        hm: homerow_mods {
            compatible = "zmk,behavior-hold-tap";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "tap-preferred";
            require-prior-idle-ms = <125>;
            quick-tap-ms = <200>;
        };
    };

    combos {
        compatible = "zmk,combos";

        Clear_BLE_Profile {
            bindings = <&bt BT_CLR>;
            key-positions = <0 38 5>;
        };

        space {
            bindings = <&kp SPACE>;
            key-positions = <18 19>;
        };

        enter {
            bindings = <&kp ENTER>;
            key-positions = <17 16>;
        };

        return {
            bindings = <&kp RET>;
            key-positions = <21 22>;
        };

        caps {
            bindings = <&kp CAPS>;
            key-positions = <12 13>;
        };

        escape {
            bindings = <&kp ESC>;
            key-positions = <1 2>;
        };

        underline {
            bindings = <&kp UNDER>;
            key-positions = <8 7>;
        };

        dash {
            bindings = <&kp MINUS>;
            key-positions = <7 6>;
        };

        backslsh {
            bindings = <&kp BSLH>;
            key-positions = <33 34>;
        };

        reload {
            bindings = <&kp LC(R)>;
            key-positions = <4 5>;
        };

        undo {
            bindings = <&kp LC(Z)>;
            key-positions = <26 25>;
        };

        cut {
            bindings = <&kp LC(X)>;
            key-positions = <26 27>;
        };

        copy {
            bindings = <&kp LC(C)>;
            key-positions = <27 28>;
        };

        paste {
            bindings = <&kp LC(V)>;
            key-positions = <28 29>;
        };

        redo {
            bindings = <&kp LC(Y)>;
            key-positions = <24 25>;
        };

        select_all {
            bindings = <&kp LC(A)>;
            key-positions = <13 14>;
        };

        save {
            bindings = <&kp LC(S)>;
            key-positions = <14 15>;
        };

        all_caps {
            bindings = <&kp CAPS>;
            key-positions = <16 19>;
        };

        backspace {
            bindings = <&kp BACKSPACE>;
            key-positions = <9 10>;
        };
    };

    macros {
        email: email {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp B &kp R &kp Y &kp A &kp N &kp AT &kp P &kp E &kp C &kp J &kp O &kp DOT &kp C &kp O &kp M>;
        };

        git_status: git_status {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp S &kp T &kp A &kp T &kp U &kp S>;
        };

        git_pull: git_pull {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp P &kp U &kp L &kp L>;
        };

        git_pull_many: git_pull_many {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <
                &kp LC(R) &kp G &kp I &kp T &kp SPACE
                &kp MINUS &kp LEFT_SHIFT &kp C
                &kp SPACE &kp LBRC &kp RBRC &kp SPACE
                &kp P &kp U &kp L &kp L &kp SQT
            >;
        };

        git_push: git_push {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp P &kp U &kp S &kp H>;
        };

        git_add: git_add {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp A &kp D &kp D &kp SPACE &kp DOT>;
        };

        git_diff: git_diff {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp D &kp I &kp F &kp F>;
        };

        git_checkout: git_checkout {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp C &kp H &kp E &kp C &kp K &kp O &kp U &kp T &kp SPACE>;
        };

        git_commit: git_commit {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <
                &kp G &kp I &kp T &kp SPACE &kp C &kp O &kp M &kp M &kp I &kp T &kp SPACE 
                &kp MINUS &kp M &kp SPACE &kp DOUBLE_QUOTES &kp DOUBLE_QUOTES &kp LEFT
            >;
        };

        start_dealsuite: start_dealsuite {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <
                &kp LC(R)          // Press CTRL + R
                &kp C &kp E &kp L &kp E  // Type 'cele'
                &macro_wait_time 1000
                &kp RET            // Press Enter
                &kp N1             // Type '1'
                &kp RET            // Press Enter

                // New Terminal tab
                &macro_wait_time 2000
                &kp LC(LS(N5))
                &kp LC(R)          // Press CTRL + R
                &macro_wait_time 50
                &kp R &kp U &kp N &kp SPACE &kp D  // Type 'run d'
                &kp RET            // Press Enter

                // New Terminal tab
                &macro_wait_time 2000
                &kp LC(LS(N5))
                &kp LC(R)          // Press CTRL + R
                &macro_wait_time 50
                &kp A &kp P &kp P &kp DOT &kp P  // Type 'app.p'
                &kp RET            // Press Enter

                // New Terminal tab
                &macro_wait_time 2000
                &kp LC(LS(N5))
                &kp LC(R)          // Press CTRL + R
                &macro_wait_time 50
                &kp R &kp U &kp N &kp S &kp E // Type 'runse'
                &kp RET            // Press Enter
            >;
        };

    };

    keymap {
        compatible = "zmk,keymap";

        base_layer {
